#
# Main arguments, all optional
#
title: Phone Panel
widget_dimensions: [105, 115]
widget_size: [1, 1]
widget_margins: [2, 2]
scalable: true
columns: 7
global_parameters:
  use_comma: 0 
  precision: 1 
  use_hass_icon: 1
  namespace: default
  devices:
    media_player:
      step: 5

layout:
  - move_pl_classic_rewind, move_pl_classic_vinyl, move_pl_deep_tracks, move_pl_grateful_dead, move_pl_tom_petty_radio, party_lights, move_volume_up
  - move_pl_80s_on_8, move_pl_90s_on_9, move_pl_poprocks, move_pl_unplugged, move_pl_bluesville, move_pl_dave_matthews, move_volume_down
  - move_pl_peter_gabriel_radio, move_pl_imagine_dragons_radio, move_pl_jimmy_buffett, move_add_family_room, drop_family_room, reload, move_turn_off
  - 

party_lights:
  widget_type: group
  entity: group.party_lights
  title: Party Lights
  icon_on: fa-toggle-on
  icon_off: fa-toggle-off

move_label:
  widget_type: label
  title: Move

# Lighting
#
accent_lighting:
  widget_type: group
  entity: group.accent_lighting
  title: Accent Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

cabinet_lighting:
  widget_type: group
  entity: group.cabinet_lighting
  title: Cabinet Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

foyer_outlet_switch_2:
  widget_type: switch
  entity: switch.foyer_outlet_switch_2
  title: Foyer Lamp
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

morning_room_lamp:
  widget_type: switch
  entity: switch.morning_room_lamp_switch
  title: Morning Room Lamp
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

landscape_lights:
  widget_type: group
  entity: group.landscape_lights
  title: Landscape Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

move_light_level:
  widget_type: light
  title: Move Light Level
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb
  units: " %"
  precision: 0
  shorten: 1
  entity: light.move_light_level

# Media
#
move_volume_up:  
  title: Volume Up
  icon_on: fa-volume-up
  icon_off: fa-volume-up
  widget_type: script
  entity: "script.move_volume_up"

move_volume_down:  
  title: Volume down
  icon_on: fa-volume-down
  icon_off: fa-volume-down
  widget_type: script
  entity: "script.move_volume_down"

move_toggle_mute:  
  title: Mute
  icon_on: mdi-volume-off
  icon_off: mdi-volume-off
  widget_type: script
  entity: "script.move_toggle_mute"

move_turn_off:
  title: Turn Off
  title2: Move 
  icon_on: mdi-power-on
  icon_off: mdi-power
  widget_type: script
  entity: "script.move_turn_off"


# Grouping Options
#  

move_add_family_room:
  title: Add FR
  title2: (To Move)
  icon_on: mdi-speaker
  icon_off: mdi-speaker
  widget_type: script
  entity: "script.move_add_family_room"

drop_family_room:
  title: Drop Family Room
  widget_type: script
  icon_on: mdi-speaker-off
  icon_off: mdi-speaker-off
  entity: "script.drop_family_room"
  
family_room_add_garage:
  title: Add Garage
  title2: (To Family Room)
  icon_on: mdi-speaker
  icon_off: mdi-speaker
  widget_type: script
  entity: "script.family_room_add_garage"

drop_garage:
  title: Drop Garage
  widget_type: script
  icon_on: mdi-speaker-off
  icon_off: mdi-speaker-off
  entity: "script.family_room_drop_garage"

office_sonos:
  widget_type: combosonos
  entity: media_player.office
  truncate_name: 64
  title: Office

move_sonos:
  widget_type: combosonos
  entity: media_player.move
  truncate_name: 64
  title: Move

family_room_sonos:
  widget_type: combosonos
  entity: media_player.family_room
  truncate_name: 64
  title: Family Room

garage_sonos:
  widget_type: combosonos
  entity: media_player.garage
  truncate_name: 64
  title: Garage  

# Misc
#
clock:
  widget_type: clock
  show_seconds: 1

family_room_label:
  widget_type: label
  title: Sonos!
  text: Family Room

# Garage Doors
#

big_garage_door:
  widget_type: cover
  entity: cover.big_door
  title: Garage
  title2: Big Door
  icon_on: mdi-garage-open
  icon_off: mdi-garage
  state_text: 1

small_garage_door:
  widget_type: cover
  entity: cover.small_door
  title: Garage
  title2: Small Door
  icon_on: mdi-garage-open
  icon_off: mdi-garage
  state_text: 1

# Climate
#
upstairs_nest:
  widget_type: nest
  nest_entity: climate.upstairs
  state_entity: sensor.upstairs_thermostat_hvac_state
  fan_entity: binary_sensor.upstairs_thermostat_fan
  title: Upstairs
  step: 1
  units: "F"
  precision: 0

downstairs_nest:
  widget_type: nest
  nest_entity: climate.downstairs
  state_entity: sensor.downstairs_thermostat_hvac_state
  fan_entity: binary_sensor.downstairs_thermostat_fan
  title: Downstairs
  step: 1
  units: "F"
  precision: 0  


# Subpanel Nav
#  
nav_to_weather:
  widget_type: navigate
  title: Weather
  title2: Dashboard
  dashboard: weather
  timeout: 8
  return: move
  icon_inactive: mdi-weather-partlycloudy
  icon_active: mdi-weather-sunny

nav_to_kitchen:
  widget_type: navigate
  title: Kitchen
  title2: Dashboard
  dashboard: kitchen
  icon_inactive: mdi-fridge
  icon_active: mdi-fridge
  
weather_now:
  widget_type: weather

reload:
  widget_type: reload
  title: Reload

