#
# Main arguments, all optional
#
title: Kitchen Panel
widget_dimensions: [120, 110]
widget_size: [1, 1]
widget_margins: [2, 2]
scalable: true
columns: 10
global_parameters:
  use_comma: 0 
  precision: 1 
  use_hass_icon: 1
  namespace: default
  devices:
    media_player:
      step: 5

layout:
  - clock(2x1), morning_room_lamp, cabinet_lighting, kitchen_light_level, liquor_cabinet_toggle, foyer_outlet_switch_2, accent_lighting, party_lights, landscape_lights # these are broken for now, big_garage_door, small_garage_door
  # holiday_lights, holiday_lights_family_room
  - weather_now(2x2), family_room_sonos(2x2), patio_sonos(2x2), garage_sonos(2x2), family_room_volume_up(2x1)
  - family_room_toggle_mute(2x1)
  - upstairs_nest(2x1), family_room_previous_track, family_room_next_track, family_room_add_patio, drop_patio, family_room_add_garage, drop_garage, family_room_volume_down(2x1)
  - downstairs_nest(2x1), family_room_turn_off(2x1), family_room_pl_unplugged, family_room_pl_fall_out_boy, family_room_pl_peter_gabriel_radio, family_room_pl_imagine_dragons_radio, family_room_pl_jimmy_buffett, family_room_pl_70s_on_7
  - family_room_pl_80s_on_8, family_room_pl_90s_on_9, family_room_pl_rockbar, family_room_pl_real_jazz, family_room_pl_bluesville, family_room_pl_dave_matthews, spacer, family_room_pl_holiday_traditions, family_room_pl_holly, family_room_apple_tv
  - family_room_pl_classic_rewind, family_room_pl_grateful_dead, family_room_pl_classic_vinyl, family_room_pl_deep_tracks, family_room_pl_tom_petty_radio, family_room_pl_yacht_rock, nav_to_patio, nav_to_weather, nav_to_garage, reload

# Lighting
#
accent_lighting:
  widget_type: group
  entity: group.accent_lighting
  title: Accent Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

holiday_lights:
  widget_type: group
  entity: group.holiday_all
  title: Holiday Lights
  icon_on: mdi-candle
  icon_off: mdi-candle

party_lights:
  widget_type: group
  entity: group.party_lights
  title: Party Lights
  icon_on: fa-toggle-on
  icon_off: fa-toggle-off

family_room_apple_tv:
  widget_type: switch
  entity: switch.family_room_apple_tv_switch
  title: Family Room AppleTV
  icon_on: mdi-apple
  icon_off: mdi-apple

holiday_lights_family_room:
  widget_type: group
  entity: group.holiday_lighting_family_room
  title: Tree & Garland
  icon_on: fa-tree
  icon_off: fa-tree
  
cabinet_lighting:
  widget_type: group
  entity: group.cabinet_lighting
  title: Cabinet Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

foyer_outlet_switch_2:
  widget_type: switch
  entity: switch.foyer_outlet_switch_2
  title: Foyer Lamp
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb
  
liquor_cabinet_toggle:
  widget_type: switch
  entity: switch.dining_room_lamp_switch_3
  title: Liquor Cabinet
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

morning_room_lamp:
  widget_type: switch
  entity: switch.morning_room_lamp_switch
  title: Morning Room Lamp
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

landscape_lights:
  widget_type: group
  entity: group.landscape_lights
  title: Landscape Lighting
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb

kitchen_light_level:
  widget_type: light
  title: Kitchen Light Level
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb
  units: " %"
  precision: 0
  shorten: 1
  entity: light.kitchen_light_level

office_strip_level:
  widget_type: light
  title: Light Strip Light Level
  icon_on: mdi-lightbulb-on
  icon_off: mdi-lightbulb
  units: " %"
  precision: 0
  shorten: 1
  entity: light.hue_lightstrip_plus_1

# Media
#
family_room_volume_up:  
  title: Volume Up
  icon_on: fa-volume-up
  icon_off: fa-volume-up
  widget_type: script
  entity: "script.family_room_receiver_volume_up"

family_room_volume_down:  
  title: Volume down
  icon_on: fa-volume-down
  icon_off: fa-volume-down
  widget_type: script
  entity: "script.family_room_receiver_volume_down"

family_room_toggle_mute:  
  title: Mute
  icon_on: mdi-volume-off
  icon_off: mdi-volume-off
  widget_type: script
  entity: "script.family_room_receiver_toggle_mute"

family_room_turn_off:
  title: Turn Off
  title2: Family Room
  icon_on: mdi-power-on
  icon_off: mdi-power
  widget_type: script
  entity: "script.family_room_turn_off"

family_room_next_track:  
  title: Next Track
  icon_on: fa-step-forward
  icon_off: fa-step-forward
  widget_type: script
  entity: "script.family_room_sonos_next_track"

family_room_previous_track:  
  title: Previous Track
  icon_on: fa-step-backward
  icon_off: fa-step-backward
  widget_type: script
  entity: "script.family_room_sonos_previous_track"

# Grouping Options
#  

family_room_add_patio:
  title: Add Patio
  title2: (To Family Room)
  icon_on: mdi-speaker
  icon_off: mdi-speaker
  widget_type: script
  entity: "script.family_room_add_patio"

drop_patio:
  title: Drop Patio
  widget_type: script
  icon_on: mdi-speaker-off
  icon_off: mdi-speaker-off
  entity: "script.drop_patio"
  
family_room_add_garage:
  title: Add Garage
  title2: (To Family Room)
  icon_on: mdi-speaker
  icon_off: mdi-speaker
  widget_type: script
  entity: "script.family_room_add_garage"

drop_garage:
  title: Drop Garage
  widget_type: script
  icon_on: mdi-speaker-off
  icon_off: mdi-speaker-off
  entity: "script.drop_garage"

office_sonos:
  widget_type: media_player
  entity: media_player.office
  truncate_name: 64
  title: Office
  dump_capabilities: 1

family_room_sonos:
#  widget_type: media_player
  widget_type: media_player
  entity: media_player.family_room
  truncate_name: 64
  title: Family Room

patio_sonos:
  widget_type: media_player
  entity: media_player.patio
  truncate_name: 64
  title: Patio

garage_sonos:
  widget_type: media_player
  entity: media_player.garage
  truncate_name: 64
  title: Garage  

# Misc
#
clock:
  widget_type: clock
  show_seconds: 1

family_room_label:
  widget_type: label
  title: Sonos!
  text: Family Room

# Garage Doors
#
big_garage_door:
  widget_type: cover
  entity: cover.big_door
  title: Garage
  title2: Big Door
  icon_on: mdi-garage-open
  icon_off: mdi-garage
  state_text: 1

small_garage_door:
  widget_type: cover
  entity: cover.small_door
  title: Garage
  title2: Small Door
  icon_on: mdi-garage-open
  icon_off: mdi-garage
  state_text: 1

# Climate
#
upstairs_nest:
  widget_type: nest
  nest_entity: climate.upstairs
  state_entity: sensor.upstairs_thermostat_hvac_state
  fan_entity: binary_sensor.upstairs_thermostat_fan
  title: Upstairs
  step: 1
  units: "F"
  precision: 0

downstairs_nest:
  widget_type: nest
  nest_entity: climate.downstairs
  state_entity: sensor.downstairs_thermostat_hvac_state
  fan_entity: binary_sensor.downstairs_thermostat_fan
  title: Downstairs
  step: 1
  units: "F"
  precision: 0  


# Subpanel Nav
#  
nav_to_weather:
  widget_type: navigate
  title: Weather
  title2: Dashboard
  dashboard: weather
#  url: https://www.wunderground.com/hourly/us/nc/matthews/28104?cm_ven=localwx_hour
  timeout: 30
  return: kitchen
  icon_inactive: mdi-weather-partlycloudy
  icon_active: mdi-weather-sunny

nav_to_garage:
  widget_type: navigate
  title: Garage
  title2: Dashboard
  dashboard: garage
  timeout: 5
  return: kitchen
  icon_inactive: mdi-garage
  icon_active: mdi-garage
  sticky: 1

nav_to_patio:
  widget_type: navigate
  title: Patio
  title2: Dashboard
  dashboard: patio
  timeout: 5
  return: kitchen
  icon_inactive: fa-leaf
  icon_active: fa-leaf
  sticky: 1

weather_now:
  widget_type: weather

reload:
  widget_type: reload
  title: Reload
  icon_active: fa-sync-alt
  icon_inactive: fa-sync-alt

